#tag ClassProtected Class BoundsRectangle	#tag Method, Flags = &h0		Sub Constructor(l as Integer = 0, t as Integer = 0, w as Integer = 0, h as Integer = 0)		  Left = l		  Top = t		  Width = w		  Height = h		  		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Function Intersects(AnotherRectangle as BoundsRectangle) As Boolean		  dim condition1, condition2, condition3, condition4 as boolean = false		  		  condition1 = anotherRectangle.Left > Left + Width		  condition2 = (anotherRectangle.Left + anotherRectangle.Width) < Left		  condition3 = anotherRectangle.Top > Top + Height		  condition4 = (anotherRectangle.Top + anotherRectangle.Height) < Top		  		  if condition1 or condition2 or condition3 or condition4 then		    return false		  else		    return true		  end if		  		End Function	#tag EndMethod	#tag Method, Flags = &h0		Function IsInsideRectangle(AnotherRectangle as BoundsRectangle) As Boolean		  return Left >= anotherRectangle.Left and Left <= anotherRectangle.Left + anotherRectangle.Width and Left + Width <= anotherRectangle.Left + _		  anotherRectangle.Width and top >= anotherRectangle.Top and top <= anotherRectangle.Top + anotherRectangle.Height and Top + _		  Height <= anotherRectangle.Top + anotherRectangle.Height		  		End Function	#tag EndMethod	#tag Method, Flags = &h0		Function IsPointInside(X as Integer, Y as Integer) As Boolean		  return x >= Left and x <= Left + Width and y >= Top and y <= Top + Height		  		End Function	#tag EndMethod	#tag Property, Flags = &h0		Height As Integer	#tag EndProperty	#tag Property, Flags = &h0		Left As Integer	#tag EndProperty	#tag Property, Flags = &h0		Top As Integer	#tag EndProperty	#tag Property, Flags = &h0		Width As Integer	#tag EndProperty	#tag ViewBehavior		#tag ViewProperty			Name="height"			Group="Behavior"			InitialValue="0"			Type="Integer"		#tag EndViewProperty		#tag ViewProperty			Name="Index"			Visible=true			Group="ID"			InitialValue=